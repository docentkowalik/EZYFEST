                    </div>
                    <!-- END Horizontal Form Title -->



                    <!-- Horizontal Form Content -->
                    <div class="block-content">

<form role="form" action='/processEditAdmin' name='processEditAdmin' id='processEditAdmin' method='POST' id="reg-form">

 <% adminData.forEach(function(admin){ %>
                        <div class="form-group">
                            <input type="text" name="first" placeholder="First name..." class="form-first-name form-control" id="first" required autofocus value="<%=admin.user_first_name %>" 
                            pattern=".{2,}"   required title="2 characters minimum">
                        </div>

                        <div class="form-group">
                            <input type="text" name="last" placeholder="Last name..." class="form-first-name form-control" id="last" required autofocus value="<%=admin.user_last_name %>"
                            pattern=".{2,}"   required title="2 characters minimum">
                        </div>

                        <div class="form-group">
                            <input type="text" name="username" placeholder="Username..." class="form-first-name form-control" id="username" required autofocus value="<%=admin.user_username %>"
                            pattern=".{2,}"   required title="2 characters minimum">
                        </div>

                        <div class="form-group">
                            <input type="email" name="emailAdd" placeholder="Email..." class="form-first-name form-control" id="emailAdd" value="<%=admin.user_email %>" readonly>
                        </div>

                                    
                                                               <button type="submit" class="btn reg">Save</button>

    <% }) %>
                            
                        </form>

        
