        <script type="text/javascript">
    
    (function( $ ) {
    $.sortByDate = function( elements, order ) {
        console.log("sort");
        var arr = [];
        elements.each(function() {
            var obj = {},
                $el = $( this ),
                time = $el.find( ".sorting" ).text(),
                date = new Date( $.trim( time ) ),
                timestamp = date.getTime();
                
                obj.html = $el[0].outerHTML;
                obj.time = timestamp;
                
                arr.push( obj );
        });
        
        var sorted = arr.sort(function( a, b ) {
            
            if( order == "ASC" ) {
                return a.time > b.time;
            } else {
                return b.time > a.time;
            }
            
        });
        
        return sorted;
    };
    
    $(function() {
        var $newer = $( "#newer" ),
            $older = $( "#older" ),
            $content = $( "tbody" ),
            $elements = $( ".contentTable" );
            
            
            
            $older.ready(function() {
                var elements = $.sortByDate( $elements, "ASC" );
                var html = "";
                for( var i = 0; i < elements.length; ++i ) {
                    html += elements[i].html;
                }
                $content[0].innerHTML = html;
                $( this ).addClass( "selected" ).
                siblings().
                removeClass( "selected" );
                return false;
                
            });
                        $classes = $( ".contentTable" ).removeClass( "post" );;

    });
    
})( jQuery );  


</script>

