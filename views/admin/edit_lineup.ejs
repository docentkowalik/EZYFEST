                <!-- END Breadcrumb -->

                <!-- Input Sizes -->
            <form role="form" action='/processEdit_lineup' name='processEdit_lineup' id='processEdit_lineup' class="form-horizontal" enctype="multipart/form-data" method='POST'>
                        <% result.forEach( function (lineup){ %>

                                <h4 class="sub-header">Artist</h4>
                                <div class="form-group">
                                    <div class="col-md-10">
                                        <label class="control-label col-md-2" for="file">Change/Upload image</label>
                                         
                                        
                                            <div class="col-md-4">
                                                <input type="file" name='lineup_artist_img' id='lineup_artist_img' class="form-control">
                                            </div>
                                             <% if(lineup.lineup_artist_img != null) { %>
                                     
                                            <div class="col-md-1">
                                                <img class="imgPlugin" src="<%= lineup.lineup_artist_img %>" alt="logo">
                                            </div>

                                            <% } %>
                                    </div>
                                    
                                    <div class="col-md-10">
                                        <label class="control-label col-md-2" for="file">Artist Name</label>
                                            <div class="col-md-4">
                                                <input type="text" name='lineup_artist_name' id='lineup_artist_name' value="<%= lineup.lineup_artist_name %>" class="form-control input-sm" placeholder="Please enter line up artist name here" maxlength="20">
                                                <input type="hidden" id="page_id" name="page_id" value="<%= lineup._id %>">

                                            </div>
                                    </div>

                                    <div class="col-md-10">
                                        <label class="control-label col-md-2" for="file">Venue</label>
                                            <div class="col-md-4">
                                                <input type="text" id="lineup_venue" name="lineup_venue" value="<%= lineup.lineup_venue %>" class="form-control input-sm"
                                                placeholder="Please enter line up venue here" maxlength="20">
                                            </div>
                                    </div>
                                    
                                    <div class="col-md-10">
                                        <h4 class="sub-header">Social Media Links <small>(optional)</small></h4>
                                    </div>
                                    <div class="col-md-10">
                                        <label class="control-label col-md-2" for="file">Facebook</label>
                                            <div class="col-md-4">
                                               <input type="text" class="form-control input-sm" name='lineup_facebook_link' id='lineup_facebook_link' value="<%= lineup.lineup_facebook_link %>"
                                               placeholder="Please enter Facebook link here">
                                            </div>
                                    </div>
                                    <div class="col-md-10">
                                        <label class="control-label col-md-2" for="file">Twitter</label>
                                            <div class="col-md-4">
                                               <input type="text" class="form-control input-sm" name='lineup_twitter_link' id='lineup_twitter_link' value="<%= lineup.lineup_twitter_link %>"
                                               placeholder="Please enter Twitter link here">
                                            </div>
                                    </div>
                                    <div class="col-md-10">
                                        <label class="control-label col-md-2" for="file">Google +</label>
                                            <div class="col-md-4">
                                               <input type="text" class="form-control input-sm" name='lineup_google_link' id='lineup_google_link' value="<%= lineup.lineup_google_link %>"
                                               placeholder="Please enter Google + link here">
                                            </div>
                                    </div>
                                    <div class="col-md-10">
                                        <label class="control-label col-md-2" for="file">Youtube</label>
                                            <div class="col-md-4">
                                               <input type="text" class="form-control input-sm" name='lineup_youtube_link' id='lineup_youtube_link' value="<%= lineup.lineup_youtube_link %>"
                                               placeholder="Please enter Youtube link here">
                                            </div>
                                    </div>
                                                                      
                                    <div class="col-md-10">
                                            <div class="col-md-4 col-md-offset-2">
                                              <button type="submit" class="btn btn-success"><i class="gi gi-check" id="icon-margin"></i>Save</button>
                                            </div>
                                    </div>
                                </div>
                                                        <% }); %>

                            </form>

                                 
