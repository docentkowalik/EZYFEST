<section id="title" class="alizarin">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <h1>Programme</h1>
                                     
                </div>
                <div class="col-sm-12">
                    
                </div>
            <br/>

 <section id="programme" class="container">

<div id="filters">
    <a href="#" id="older" class="selected">Older</a>
    <a href="#" id="newer">Newer</a>
</div>

    <div class="container">
        <div class="row">
            <div class="col-lg-10 col-lg-offset-1">
                <ul class="event-list">
                
                <% for(var i=0; i<programmeOBJ.length; i++) {%>


                    <li class="post">
                        <time datetime="2015-01-20 0000">
                            <span class="day"><%= programmeOBJ[i].event_day %></span>
                            <span class="month"><%= programmeOBJ[i].programme_date %></span>
                        </time>
                        <div class="info">
                            <h2 class="title">Artist: <%= programmeOBJ[i].event_artist_name %></h2>
                            <p class="desc">Venue: <%= programmeOBJ[i].event_venue %></p>
                            <ul>
                                <li style="width:50%;"><span class="fa fa-clock-o margin" ></span><%= programmeOBJ[i].event_time_start %> - <%= programmeOBJ[i].event_time_end %></li>
                                <li style="width:50%;" class="date"><span class="fa fa-calendar-o margin"></span><%= programmeOBJ[i].programme_date %></li>
                            </ul>
                        </div>
                        
                    </li>
                
                <% } %>

                </ul>
            </div>
        </div>
    </div> 
    </section><!--/#programme-->
</div>
        </div>
    </section><!--/#title--> 

                        

